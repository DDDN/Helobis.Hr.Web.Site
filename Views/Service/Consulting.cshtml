@using Helobis.Hr.Web.Site.Resources

@{
    Layout = "~/Views/Shared/_LayoutDefault.cshtml";
}

@Html.Partial("_PartialCarousel")

<section class="container service-section" id="consulting-section">
    <div class="inner">
        <div class="row">
            <div class="col-sm-12">
                <h1>@ResHeaders.ConsultingConsulting</h1>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-6 service-content text-justify">
                <h4>@ResHeaders.Characteristic</h4>
                <p>@ResConsulting.CharacteristicsText1</p>
                <p>@ResConsulting.CharacteristicsText2</p>
                <h4>@ResHeaders.Advantages</h4>
                <p>@ResConsulting.AdvantagesText1</p>
                <h4>@ResHeaders.Benefits</h4>
                <p>@ResConsulting.BenefitsText1</p>
            </div>
            <div class="col-sm-6 service-content">
                <h4>@ResConsulting.ExamplesHeader</h4>
                <div class="service-content-list full-width">
                    <h4>@ResConsulting.Example1Header</h4>
                    <ul class="fa-ul ">
                        <li><h5><i class="fa-li fa fa-check-square"></i>@ResConsulting.Example1Item1</h5></li>
                        <li><h5><i class="fa-li fa fa-check-square"></i>@ResConsulting.Example1Item2</h5></li>
                        <li><h5><i class="fa-li fa fa-check-square"></i>@ResConsulting.Example1Item3</h5></li>
                        <li><h5><i class="fa-li fa fa-check-square"></i>@ResConsulting.Example1Item4</h5></li>
                        <li><h5><i class="fa-li fa fa-check-square"></i>@ResConsulting.Example1Item5</h5></li>
                        <li><h5><i class="fa-li fa fa-check-square"></i>@ResConsulting.Example1Item6</h5></li>
                    </ul>
                </div>
                <div class="service-content-list full-width">
                    <h4>@ResConsulting.Example2Header</h4>
                    <ul class="fa-ul ">
                        <li><h5><i class="fa-li fa fa-check-square"></i>@ResConsulting.Example2Item1</h5></li>
                        <li><h5><i class="fa-li fa fa-check-square"></i>@ResConsulting.Example2Item2</h5></li>
                        <li><h5><i class="fa-li fa fa-check-square"></i>@ResConsulting.Example2Item3</h5></li>
                        <li><h5><i class="fa-li fa fa-check-square"></i>@ResConsulting.Example2Item4</h5></li>
                        <li><h5><i class="fa-li fa fa-check-square"></i>@ResConsulting.Example2Item5</h5></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</section>